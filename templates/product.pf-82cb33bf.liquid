$(document).ready(function(){
  
  
  var learnpac_getUrlParameter = function learnpac_getUrlParameter(sParam) {
    var sPageURL = window.location.search.substring(1),
        sURLVariables = sPageURL.split('&'),
        sParameterName,
        i;
if(sURLVariables){
    for (i = 0; i < sURLVariables.length; i++) {
        sParameterName = sURLVariables[i].split('=');

        if (sParameterName[0] === sParam) {
            return typeof sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
        }
    }
}
    return false;
};
  
  let incoming_parameter = learnpac_getUrlParameter('variant');
  if(incoming_parameter){
	$('.learnpac_seats_remaining').html('<div>'+window.__pagefly_variant_stock[parseInt(incoming_parameter)]+' seats remaining</div>');	
  }else{
	$('.learnpac_seats_remaining').html('<div>'+$('input[data-pf-type="QuantityField"]').attr('max')+' seats remaining</div>');
  }
